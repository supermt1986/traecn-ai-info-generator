{
  "name": "Traecn-ai-info-generator",
  "version": "1.0.0",
  "description": "CloudFlare平台AI平台管理系统",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm ci && npm run build",
    "build:server": "tsc",
    "db:migrate": "wrangler d1 migrations apply ai-info-generator-db",
    "setup": "npm install && cd client && npm install"
  },
  "dependencies": {
    "hono": "^4.6.0",
    "@hono/node-server": "^1.13.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.22.1",
    "vite": "^5.0.10"
  },
  "keywords": ["cloudflare", "workers", "ai", "management"],
  "author": "Traecn",
  "license": "MIT"
}